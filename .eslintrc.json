{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": ["error", { "code": 80 }],
    "space-before-function-paren": "off"
  }
}
